{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "globals": {
        "TreeNode": "readonly",
        "ListNode": "readonly"
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": [
            "error",
            {
                "code": 80
            }
        ],
        "no-var": "off"
    }
}